<div class="panel panel-primary panel-inverse">
  <div class="panel-heading">
    <h4>Popular Links</h4>
  </div>
  <div class="panel-body">
    <ul class="list-group">
    <% @popular_links.each.with_index(1) do |link, index| %>
    <li class="presentation list-group-item">
      <span class="badge"><%= index %></span>
      <%= link_to link.shortened_url, link.original, target: '_blank' %>
    </li>
    <% end %>
    </ul>
  </div>
</div>
    
    
